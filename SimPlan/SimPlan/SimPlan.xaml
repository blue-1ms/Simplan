<Window x:Name="SimPlan" x:Class="SimPlan.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimPlan"
        mc:Ignorable="d"
        Title="SimPlan" Height="450" Width="800">

    <Grid>

        <Label Content="Welcome To SimPlan" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="330,157,0,0"/>
        <Button x:Name="Previous" Margin="220,225,406,124" Background="#FF46FF00" FontFamily="Calibri" Click="Click_Prev">Open Previous Plan</Button>
        <Button x:Name="New" Margin="431,225,195,124" Background="#FF46FF00" FontFamily="Calibri" Click="Click_New">Start New Plan</Button>
        <Image Margin="262,16,278,267" Source="Image1.png" Stretch="Fill" OpacityMask="Black">
            <Image.Effect>
                <DropShadowEffect/>
            </Image.Effect>
        </Image>
    </Grid>
</Window>
